{
    "name": "lucadegasperi/oauth2-server-laravel",
    "description": "A Laravel 4 wrapper for the popular OAuth2 server by php-loep",
    "keywords": [
        "laravel",
        "oauth2",
        "oauth",
        "server"
    ],
    "authors": [
        {
            "name": "Luca Degasperi",
            "email": "packages@lucadegasperi.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "laravel/framework": "4.0.x",
        "league/oauth2-server": "2.1.x"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Lucadegasperi\\OAuth2Server": "src/"
        }
    },
    "minimum-stability": "dev",
    "license": "MIT"
}